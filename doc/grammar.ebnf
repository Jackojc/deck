comment ::= ? ;.*$ ?
identifier ::= ? {XID_Start}{XID_Continue}* ?
number ::= ? [0-9]+ ? | ? 0x[0-9A-Fa-f]+ ?

addr ::= '&' <identifier>
workspace ::= '[' <instruction>* ']'
label ::= '@' <identifier>

instruction ::=
	<workspace> | <label> | <identifier> | <addr> |
	'+' | '-' | '*' | '/' | '%'

function ::= <instruction>* '::' <identifier>
program ::= ( <function> | <instruction> )*
